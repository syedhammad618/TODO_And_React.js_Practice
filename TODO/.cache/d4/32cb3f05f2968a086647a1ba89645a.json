rent.focus();\n  }\n\n  render() {\n    return (\n      <div>\n        <form className=\"formedit\" onSubmit={this.value_handler}>\n          <div>\n            <input\n              type=\"text\"\n              name=\"str\"\n              ref={this.textinput}\n              id={this.state.id}\n            />\n            <input className=\"button\" type=\"submit\" value=\"Add\" />\n          </div>\n        </form>\n        <List lit={this.state.items} handleDelete={this.handleDelete} />\n      </div>\n    );\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"bc8f1f89bda912eeaf464ce492611c89","cacheData":{"env":{}}}